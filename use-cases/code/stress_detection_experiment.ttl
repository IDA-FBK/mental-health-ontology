@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ex: <https://w3id.org/miao/experiment#> .
@prefix miao: <https://w3id.org/miao#> .
@prefix mls: <http://www.w3.org/ns/mls#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:PyTorch a mls:Software ;
    rdfs:label "PyTorch"@en ;
    dcterms:description "Open-source machine learning framework"@en ;
    mls:hasPart ex:BERT_Stress_Classifier_Implementation .

ex:4b34daf2_accuracy_evaluation a mls:ModelEvaluation ;
    mls:hasValue "0.85"^^xsd:float ;
    mls:specifiedBy ex:accuracy .

ex:4b34daf2_f1_score_evaluation a mls:ModelEvaluation ;
    mls:hasValue "0.85"^^xsd:float ;
    mls:specifiedBy ex:f1_score .

ex:4b34daf2_precision_evaluation a mls:ModelEvaluation ;
    mls:hasValue "0.83"^^xsd:float ;
    mls:specifiedBy ex:precision .

ex:4b34daf2_recall_evaluation a mls:ModelEvaluation ;
    mls:hasValue "0.88"^^xsd:float ;
    mls:specifiedBy ex:recall .

ex:BERT_Stress_Classifier_batch_size a mls:HyperParameter ;
    rdfs:label "Batch Size"@en ;
    mls:hasValue 32 .

ex:BERT_Stress_Classifier_epochs a mls:HyperParameter ;
    rdfs:label "Epochs"@en ;
    mls:hasValue 10 .

ex:BERT_Stress_Classifier_learning_rate a mls:HyperParameter ;
    rdfs:label "Learning Rate"@en ;
    mls:hasValue "2e-05"^^xsd:float .

ex:BERT_Stress_Classifier_v1.0 a mls:Model ;
    rdfs:label "BERT_Stress_Classifier v1.0"@en ;
    dcterms:created "2025-11-15"^^xsd:date ;
    dcterms:description "Trained BERT model for binary stress classification"@en .

ex:Run_4b34daf2 a mls:Run,
        miao:AutomaticMentalIllnessesDetection ;
    rdfs:label "Stress detection run 4b34daf2"@en ;
    dcterms:created "2025-12-02T00:20:02"^^xsd:dateTime ;
    dcterms:description "Automatic stress detection from text using BERT classifier"@en ;
    mls:executes ex:BERT_Stress_Classifier_Implementation ;
    mls:hasOutput ex:4b34daf2_accuracy_evaluation,
        ex:4b34daf2_f1_score_evaluation,
        ex:4b34daf2_precision_evaluation,
        ex:4b34daf2_recall_evaluation,
        ex:BERT_Stress_Classifier_v1.0 ;
    prov:generated ex:StressSet_4b34daf2 ;
    miao:hasInputData ex:StressTextDataset ;
    miao:usedMentalIllnessesSchema ex:StressSchema_Binary .

ex:StressTextDataset a mls:Dataset ;
    rdfs:label "StressTextDataset - Text samples for stress detection"@en ;
    dcterms:created "2025-12-02T00:20:02"^^xsd:dateTime ;
    dcterms:description "Dataset containing 5 text samples with binary stress annotations"@en ;
    dcterms:extent "5 samples"@en ;
    dcterms:format "text/plain" ;
    ex:noStressCount 2 ;
    ex:stressCount 3 .

ex:Stress_4b34daf2_sample_0 a ex:Stress ;
    rdfs:label "Stress prediction for sample 0"@en ;
    dcterms:description "Prediction for text: 'I'm feeling overwhelmed with work deadlines and family responsibilities...'"@en ;
    miao:belongsToMentalIllnessesSet ex:StressSet_4b34daf2 ;
    miao:hasMentalIllnessDetectionConfidence 0.92 ;
    miao:referredToMentalIllnessCategory ex:Stress_Positive ;
    miao:refersToSample "text_sample_0"^^xsd:string .

ex:Stress_4b34daf2_sample_1 a ex:Stress ;
    rdfs:label "No Stress prediction for sample 1"@en ;
    dcterms:description "Prediction for text: 'Had a great day at the beach, feeling relaxed and happy...'"@en ;
    miao:belongsToMentalIllnessesSet ex:StressSet_4b34daf2 ;
    miao:hasMentalIllnessDetectionConfidence 0.87 ;
    miao:referredToMentalIllnessCategory ex:Stress_Negative ;
    miao:refersToSample "text_sample_1"^^xsd:string .

ex:Stress_4b34daf2_sample_2 a ex:Stress ;
    rdfs:label "Stress prediction for sample 2"@en ;
    dcterms:description "Prediction for text: 'Can't sleep at night, constant worry about finances...'"@en ;
    miao:belongsToMentalIllnessesSet ex:StressSet_4b34daf2 ;
    miao:hasMentalIllnessDetectionConfidence 0.78 ;
    miao:referredToMentalIllnessCategory ex:Stress_Positive ;
    miao:refersToSample "text_sample_2"^^xsd:string .

ex:Stress_4b34daf2_sample_3 a ex:Stress ;
    rdfs:label "No Stress prediction for sample 3"@en ;
    dcterms:description "Prediction for text: 'Enjoyed a peaceful morning walk in the park...'"@en ;
    miao:belongsToMentalIllnessesSet ex:StressSet_4b34daf2 ;
    miao:hasMentalIllnessDetectionConfidence 0.95 ;
    miao:referredToMentalIllnessCategory ex:Stress_Negative ;
    miao:refersToSample "text_sample_3"^^xsd:string .

ex:Stress_4b34daf2_sample_4 a ex:Stress ;
    rdfs:label "Stress prediction for sample 4"@en ;
    dcterms:description "Prediction for text: 'My anxiety is through the roof with upcoming exams...'"@en ;
    miao:belongsToMentalIllnessesSet ex:StressSet_4b34daf2 ;
    miao:hasMentalIllnessDetectionConfidence 0.84 ;
    miao:referredToMentalIllnessCategory ex:Stress_Positive ;
    miao:refersToSample "text_sample_4"^^xsd:string .

ex:accuracy a mls:EvaluationMeasure ;
    rdfs:label "Accuracy"@en .

ex:f1_score a mls:EvaluationMeasure ;
    rdfs:label "F1 Score"@en .

ex:precision a mls:EvaluationMeasure ;
    rdfs:label "Precision"@en .

ex:recall a mls:EvaluationMeasure ;
    rdfs:label "Recall"@en .

ex:BERT_Stress_Classifier_Implementation a mls:Implementation ;
    rdfs:label "BERT_Stress_Classifier Implementation"@en ;
    dcterms:description "BERT-based classifier fine-tuned for stress detection"@en ;
    mls:hasHyperParameter ex:BERT_Stress_Classifier_batch_size,
        ex:BERT_Stress_Classifier_epochs,
        ex:BERT_Stress_Classifier_learning_rate .

ex:StressSchema_Binary a miao:MentalIllnessesSchema ;
    dcterms:created "2025-12-02"^^xsd:date ;
    dcterms:description "Binary schema for computational stress detection from text"@en ;
    dcterms:title "Binary Stress Classification Schema"@en ;
    miao:hasMentalIllnessCategory ex:Stress_Negative,
        ex:Stress_Positive .

ex:Stress_Negative a miao:MentalIllnessCategory ;
    dcterms:description "No psychological stress indicators detected"@en ;
    dcterms:title "No Stress"@en ;
    miao:isMentalIllnessCategoryOf ex:StressSchema_Binary .

ex:Stress_Positive a miao:MentalIllnessCategory ;
    dcterms:description "Psychological stress indicators detected"@en ;
    dcterms:title "Stress"@en ;
    miao:isMentalIllnessCategoryOf ex:StressSchema_Binary .

ex:Stress rdfs:label "Stress"@en ;
    rdfs:subClassOf miao:MentalIllness .

ex:StressSet_4b34daf2 a miao:MentalIllnessesSet ;
    rdfs:label "Stress detection results from run 4b34daf2"@en ;
    prov:wasGeneratedBy ex:Run_4b34daf2 ;
    miao:hasMentalIllness ex:Stress_4b34daf2_sample_0,
        ex:Stress_4b34daf2_sample_1,
        ex:Stress_4b34daf2_sample_2,
        ex:Stress_4b34daf2_sample_3,
        ex:Stress_4b34daf2_sample_4 .

