    mls:hasInput ex:Stress_Detection_Dataset ;
    mls:implements ex:Model_BERT_Base ;
    prov:generated ex:ResultSet_BERT_Base ;
    miao:hasInputData ex:Stress_Detection_Dataset ;
    miao:usedMentalIllnessesSchema ex:StressSchema_Binary .

