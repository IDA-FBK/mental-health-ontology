ex:StressSchema_Binary a miao:MentalIllnessesSchema ;
    dcterms:created "2025-12-02"^^xsd:date ;
    dcterms:description "Binary schema for computational stress detection from text and multimodal signals. Used in social media analysis and real-time monitoring applications."@en ;
    dcterms:source "Turcan E, McKeown K. Dreaddit: A Reddit Dataset for Stress Analysis in Social Media. LOUHI 2019." ;
    dcterms:title "Binary Stress Classification Schema"@en ;
    miao:hasMentalIllnessCategory ex:StressNegative,
        ex:StressPositive .

ex:Stress_Detection_Dataset a mls:Dataset ;
    dcterms:created "2025-12-04"^^xsd:date ;
    dcterms:description "Text dataset with 10 samples annotated with binary stress labels (0=No Stress, 1=Stress) for computational stress detection research." ;
    dcterms:title "Stress Detection Dataset" ;
    mls:hasFeature ex:Feature_CognitiveLoadIndicators,
        ex:Feature_NegativeEmotionScore,
        ex:Feature_PhysiologicalMarkers,
        ex:Feature_TemporalUrgency,
        ex:Feature_TextContent ;
    mls:hasQuality ex:DataChar_NumberOfClasses,
        ex:DataChar_NumberOfInstances,
        ex:DataChar_StressPrevalence .

