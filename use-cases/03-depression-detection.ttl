ex:BERT_Base_Setting_batch_size a mls:HyperParameterSetting ;
    mls:hasValue 16 ;
    mls:specifiedBy ex:HyperParam_batch_size .

ex:BERT_Base_Setting_dropout_rate a mls:HyperParameterSetting ;
    mls:hasValue "0.1"^^xsd:float ;
    mls:specifiedBy ex:HyperParam_dropout_rate .

ex:BERT_Base_Setting_learning_rate a mls:HyperParameterSetting ;
    mls:hasValue "2e-05"^^xsd:float ;
    mls:specifiedBy ex:HyperParam_learning_rate .

ex:BERT_Base_Setting_max_seq_length a mls:HyperParameterSetting ;
    mls:hasValue 512 ;
    mls:specifiedBy ex:HyperParam_max_seq_length .

ex:BERT_Base_Setting_num_epochs a mls:HyperParameterSetting ;
    mls:hasValue 3 ;
    mls:specifiedBy ex:HyperParam_num_epochs .

ex:BiLSTM_Setting_max_features a mls:HyperParameterSetting ;
    mls:hasValue 10000 ;
    mls:specifiedBy ex:HyperParam_max_features .

ex:BiLSTM_Setting_min_df a mls:HyperParameterSetting ;
    mls:hasValue 5 ;
    mls:specifiedBy ex:HyperParam_min_df .

ex:BiLSTM_Setting_ngram_range a mls:HyperParameterSetting ;
    mls:hasValue "(1,2)" ;
