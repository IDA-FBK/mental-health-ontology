ex:Evaluation_BERT_Base a mls:ModelEvaluation ;
    dcterms:title "Evaluation: BERT_Base" ;
    mls:evaluates ex:Model_BERT_Base ;
    mls:specifiedBy ex:BERT_Base_Metric_accuracy,
        ex:BERT_Base_Metric_f1_score,
        ex:BERT_Base_Metric_precision,
        ex:BERT_Base_Metric_recall .

ex:Evaluation_BiLSTM a mls:ModelEvaluation ;
