    mls:hasValue "10"^^xsd:float .

ex:Detection_BERT_Base a mls:Run,
        miao:AutomaticMentalIllnessesDetection ;
    dcterms:description "Automatic detection run using BERT_Base model" ;
    dcterms:title "Depression Detection Run: BERT_Base" ;
    mls:hasInput ex:Depression_Severity_Dataset ;
    mls:implements ex:Model_BERT_Base ;
    prov:generated ex:ResultSet_BERT_Base ;
    miao:hasInputData ex:Depression_Severity_Dataset ;
    miao:usedMentalIllnessesSchema ex:DepressionSeveritySchema_Research .

