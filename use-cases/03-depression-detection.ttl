ex:DataChar_AverageTextLength a mls:DataCharacteristic ;
    rdfs:label "AverageTextLength" ;
    dcterms:description "Average character length of text" ;
    mls:hasValue "52.7"^^xsd:float .

ex:DataChar_ClassBalance a mls:DataCharacteristic ;
    rdfs:label "ClassBalance" ;
