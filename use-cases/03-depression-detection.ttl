ex:Depression_Severity_Dataset a miao:Dataset ;
    dcterms:created "2025-12-03"^^xsd:date ;
    dcterms:description "Text dataset with 10 samples annotated with depression severity labels (0-3) based on PHQ-9 criteria." ;
    dcterms:title "Depression Severity Dataset" ;
    miao:hasClassDistribution ex:Depression_Severity_Dataset_Distribution_Level0,
        ex:Depression_Severity_Dataset_Distribution_Level1,
        ex:Depression_Severity_Dataset_Distribution_Level2,
        ex:Depression_Severity_Dataset_Distribution_Level3 ;
    miao:numberOfSamples 10 ;
    miao:usesSchema ex:DepressionSeveritySchema_Research .

