ex:DepressionSeveritySchema_Research a miao:MentalIllnessesSchema ;
    dcterms:created "2025-12-02"^^xsd:date ;
    dcterms:description "Ordinal taxonomy of depression severity (Minimal, Mild, Moderate, Severe) used in social media corpora and text-based computational research on depression detection. Based on PHQ-9 severity thresholds."@en ;
    dcterms:source "Kroenke K, Spitzer RL, Williams JB. The PHQ-9: validity of a brief depression severity measure. J Gen Intern Med. 2001;16(9):606-13." ;
    dcterms:title "Research schema for depression severity in text"@en ;
    miao:hasMentalIllnessCategory ex:MildDepression,
        ex:MinimalDepression,
        ex:ModerateDepression,
        ex:SevereDepression .

ex:Depression_Severity_Dataset a mls:Dataset ;
    dcterms:created "2025-12-04"^^xsd:date ;
    dcterms:description "Text dataset with 10 samples annotated with depression severity labels (0-3) based on PHQ-9 criteria." ;
    dcterms:title "Depression Severity Dataset" ;
    mls:hasFeature ex:Feature_EmotionalIntensity,
        ex:Feature_SentimentScore,
        ex:Feature_TextContent,
        ex:Feature_TextLength ;
    mls:hasQuality ex:DataChar_AverageTextLength,
        ex:DataChar_ClassBalance,
        ex:DataChar_NumberOfClasses,
        ex:DataChar_NumberOfInstances .

