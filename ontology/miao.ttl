#################################################################
#    Object Properties
#################################################################

###  http://www.w3.org/ns/prov#generated
prov:generated rdf:type owl:ObjectProperty ;
               owl:inverseOf prov:wasGeneratedBy ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain miao:MentalIllnessesDetection ;
               rdfs:range miao:MentalIllnessesSet ;
               rdfs:comment "Relation taken from the Prov ontology that is used to represent the relation between a MentalIllnessesDetection and the generated set of MentalIllnessesSet." .


###  http://www.w3.org/ns/prov#wasGeneratedBy
prov:wasGeneratedBy rdf:type owl:ObjectProperty ,
                             owl:InverseFunctionalProperty ;
                    rdfs:domain miao:MentalIllnessesSet ;
                    rdfs:range miao:MentalIllnessesDetection ;
                    rdfs:comment "Ref: https://www.w3.org/TR/prov-o/#wasGeneratedBy" .


###  https://w3id.org/miao#belongsToMentalIllnessesSet
miao:belongsToMentalIllnessesSet rdf:type owl:ObjectProperty ;
                                 owl:inverseOf miao:hasMentalIllness ;
                                 rdfs:domain miao:MentalIllness ;
                                 rdfs:range miao:MentalIllnessesSet ;
                                 rdfs:comment "Inverse relation of hasMentalIllness. This relation connect a MentalIllness with its corresponding MentalIllnessesSet." .


###  https://w3id.org/miao#hasInputData
miao:hasInputData rdf:type owl:ObjectProperty ;
                  owl:inverseOf miao:isInputOfMentalIllnessesDetection ;
                  rdfs:domain miao:MentalIllnessesDetection ;
                  rdfs:range mls:Data ;
                  rdfs:comment "A relation between a MentalIllnessesDetection and data that is taken as input to the MentalIllnessesDetection." .


###  https://w3id.org/miao#hasMentalIllness
miao:hasMentalIllness rdf:type owl:ObjectProperty ;
                      rdfs:domain miao:MentalIllnessesSet ;
                      rdfs:range miao:MentalIllness ;
                      rdfs:comment "Relation connecting a MentalIllnessesSet with its corresponding MentalIllnesses." .


###  https://w3id.org/miao#hasMentalIllnessCategory
miao:hasMentalIllnessCategory rdf:type owl:ObjectProperty ;
                             owl:inverseOf miao:isMentalIllnessCategoryOf ;
                             rdfs:domain miao:MentalIllnessesSchema ;
                             rdfs:range miao:MentalIllnessCategory ;
                             rdfs:comment "A relation between a MentalIllnessesModel and its Mental Illness categories." .


###  https://w3id.org/miao#isInputOfMentalIllnessesDetection
miao:isInputOfMentalIllnessesDetection rdf:type owl:ObjectProperty ;
                                      rdfs:domain mls:Data ;
                                      rdfs:range miao:MentalIllnessesDetection .


###  https://w3id.org/miao#isMentalIllnessCategoryOf
miao:isMentalIllnessCategoryOf rdf:type owl:ObjectProperty ;
                              rdfs:domain miao:MentalIllnessCategory ;
                              rdfs:range miao:MentalIllnessesSchema ;
                              rdfs:comment "Inverse relation of hasMentalIllnessCategory object propery that represents the relation between a MentalIllnessCategory and its corresponding MentalIllnessesModel." .


###  https://w3id.org/miao#referredToMentalIllness
miao:referredToMentalIllness rdf:type owl:ObjectProperty ;
                             owl:inverseOf miao:referredToMentalIllnessCategory ;
                             rdf:type owl:InverseFunctionalProperty ;
                             rdfs:domain miao:MentalIllnessCategory ;
                             rdfs:range miao:MentalIllness ;
                             rdfs:comment "Inverse relation of referredToMentalIllnessCategory used to represent to which MentalIllness (predicted) a MentalIllnessCategoy (from the literature) corresponds to." .


###  https://w3id.org/miao#referredToMentalIllnessCategory
miao:referredToMentalIllnessCategory rdf:type owl:ObjectProperty ,
                                              owl:FunctionalProperty ;
                                     rdfs:domain miao:MentalIllness ;
                                     rdfs:range miao:MentalIllnessCategory ;
                                     rdfs:comment "Relation used to represent to which MentalIllnessCategoy (from the literature) a MentalIllness (predicted) corresponds to." .


###  https://w3id.org/miao#usedInMentalIllnessesDetection
miao:usedInMentalIllnessesDetection rdf:type owl:ObjectProperty ;
                                    owl:inverseOf miao:usedMentalIllnessesSchema ;
                                    rdfs:domain miao:MentalIllnessesSchema ;
                                    rdfs:range miao:MentalIllnessesDetection ;
                                    rdfs:comment "Inverse relation of usedMentalIllnessesSchema used to indicate which MentalIllnessesDetection a specific MentalIllnessesSchema (from the literature) refers to." .


###  https://w3id.org/miao#usedMentalIllnessesSchema
miao:usedMentalIllnessesSchema rdf:type owl:ObjectProperty ,
                                        owl:FunctionalProperty ;
                               rdfs:domain miao:MentalIllnessesDetection ;
                               rdfs:range miao:MentalIllnessesSchema ;
                               rdfs:comment "Relations used to indicate to which MentalIllnessesSchema (from the literature) a MentalIllnessesDetection is based on." .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/miao#hasMentalIllnessDetectionConfidence
miao:hasMentalIllnessDetectionConfidence rdf:type owl:DatatypeProperty ;
                                         rdfs:domain miao:MentalIllness ;
                                         rdfs:range xsd:decimal ;
                                         rdfs:comment "Data Property used to represent the confidence of both an human expert or a machine/deep learning model about the presence of a mental illness." .


###  https://w3id.org/miao#hasMentalIllnessLevel
miao:hasMentalIllnessLevel rdf:type owl:DatatypeProperty ;
                           rdfs:domain miao:MentalIllness ;
                           rdfs:range xsd:string ;
                           rdfs:comment "Represents the intensity of the mental illness as a categorical value, e.g., Low, Medium, or High." .


###  https://w3id.org/miao#hasMentalIllnessScore
miao:hasMentalIllnessScore rdf:type owl:DatatypeProperty ;
                           rdfs:domain miao:MentalIllness ;
                           rdfs:range xsd:decimal ;
                           rdfs:comment "Represents the intensity of the mental illness as a numeric score between 0 and 1." .


###  https://w3id.org/miao#refersToSample
miao:refersToSample rdf:type owl:DatatypeProperty ;
                    rdfs:domain miao:MentalIllness ;
                    rdfs:range xsd:string ;
                    rdfs:comment "This data property represents the unique identifier of the specific data sample on which one or more mental illnesses have been detected. It is used to link a mental illness detection result to the original collected data instance." .


#################################################################
#    Classes
#################################################################

###  http://www.w3.org/ns/mls#Data
mls:Data rdf:type owl:Class .


###  http://www.w3.org/ns/mls#Run
mls:Run rdf:type owl:Class .


###  http://www.w3.org/ns/prov#Activity
prov:Activity rdf:type owl:Class ;
              rdfs:comment "Ref: https://www.w3.org/TR/prov-o/#Activity" .


###  http://www.w3.org/ns/prov#Entity
prov:Entity rdf:type owl:Class ;
            rdfs:comment "Ref: https://www.w3.org/TR/prov-o/#Entity" .


###  https://w3id.org/miao#AutomaticMentalIllnessesDetection
miao:AutomaticMentalIllnessesDetection rdf:type owl:Class ;
                                       rdfs:subClassOf mls:Run ,
                                                       miao:MentalIllnessesDetection ;
                                       owl:disjointWith miao:ManualMentalIllnessesDetection ;
                                       rdfs:comment "An AutomaticMentalIllnessesDetection represents an analysis (i.e., a run) performed by a machine/deep learning model to detect mental illnesses from a given source." ;
                                       rdfs:label "Automatic Mental Illnesses Detection"@en ,
                                                  "Rilevamento Automatico Malattie Mentali"@it .


###  https://w3id.org/miao#ManualMentalIllnessesDetection
miao:ManualMentalIllnessesDetection rdf:type owl:Class ;
                                    rdfs:subClassOf miao:MentalIllnessesDetection ;
                                    rdfs:comment "A ManualMentalIllnessesDetection represents an analysis performed by a human expert to detect mental illnesses from a given source." ;
                                    rdfs:label "Manual Detection Mental Illnesses"@en ,
                                               "Rilevamento Manuale Malattie Mentali"@it .


###  https://w3id.org/miao#MentalIllnessesSchema
miao:MentalIllnessesSchema rdf:type owl:Class ;
                           rdfs:subClassOf [ rdf:type owl:Restriction ;
                                             owl:onProperty miao:hasMentalIllnessCategory ;
                                             owl:someValuesFrom miao:MentalIllnessCategory
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty miao:usedInMentalIllnessesDetection ;
                                             owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                                             owl:onClass miao:MentalIllnessesDetection
                                           ] ;
                           rdfs:comment "Represents a category of mental illness within a specific schema (e.g., an ontology or classification system)." ;
                           rdfs:label "Mental Illnesses Schema"@en ,
                                      "Schema Malattie Mentali"@it .


###  https://w3id.org/miao#MentalIllnessesSet
miao:MentalIllnessesSet rdf:type owl:Class ;
                        rdfs:subClassOf prov:Entity ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty miao:hasMentalIllness ;
                                          owl:someValuesFrom miao:MentalIllness
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty prov:wasGeneratedBy ;
                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass miao:MentalIllnessesDetection
                                        ] ;
                        rdfs:comment "Concept used to represent a set of detected mental illnesses." ;
                        rdfs:label "Insieme Malattie Mentali"@it ,
                                   "Mental Illnesses Set"@en .


###  https://w3id.org/miao#MentalIllness
miao:MentalIllness rdf:type owl:Class ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty miao:belongsToMentalIllnessesSet ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass miao:MentalIllnessesSet
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty miao:referredToMentalIllnessCategory ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass miao:MentalIllnessCategory
                                   ] ;
                   rdfs:comment "A MentalIllness represents a detected or predicted mental illness, typically as a result of a human or machine/deep learning analysis." ;
                   rdfs:label "Malattia Mentale"@it ,
                              "Mental Illness"@en .


###  https://w3id.org/miao#MentalIllnessCategory
miao:MentalIllnessCategory rdf:type owl:Class ;
                           rdfs:subClassOf [ rdf:type owl:Restriction ;
                                             owl:onProperty miao:referredToMentalIllness ;
                                             owl:someValuesFrom miao:MentalIllness
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty miao:isMentalIllnessCategoryOf ;
                                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                             owl:onClass miao:MentalIIlnessesSchema
                                           ] ;
                           rdfs:comment "A concept used to represent a mental illness category that belongs to a specific schema (e.g., ontology) used to represent mental illnesses" ;
                           rdfs:label "Categoria Malattia Mentale"@it ,
                                      "Mental Illness Category"@en .


###  https://w3id.org/miao#MentalIllnessesDetection
miao:MentalIllnessesDetection rdf:type owl:Class ;
                              rdfs:subClassOf prov:Activity ,
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty miao:hasInputData ;
                                                owl:someValuesFrom mls:Data
                                              ] ,
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty prov:generated ;
                                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                owl:onClass miao:MentalIllnessesSet
                                              ] ,
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty miao:usedMentalIllnessesSchema ;
                                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                owl:onClass miao:MentalIllnessesSchema
                                              ] ;
                              rdfs:comment "High-level concept used to represent a mental illness detection process regardless of the type (i.e., manual or automatic)." ;
                              rdfs:label "Mental Illnesses Detection"@en ,
                                         "Rilevamento Malattie Mentali"@it .


